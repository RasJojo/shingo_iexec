# Networks
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
ARBITRUM_SEPOLIA_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
# Optional override for iExec SDK RPC (defaults to ARBITRUM_SEPOLIA_RPC_URL then ARBITRUM_RPC_URL)
IEXEC_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
LOCAL_USE_MOCK_TOKEN=true

# Wallets
DEPLOYER_PRIVATE_KEY=0x
RELAY_PRIVATE_KEY=0x
SUBSCRIBER_PRIVATE_KEY=0x

# Payment token on Arbitrum network you deploy to
# USDC Arbitrum One:      0xaf88d065e77c8cC2239327C5EDb3A432268e5831
# USDC Arbitrum Sepolia:  0x75faf114eafb1BDbe2F0316DF893fd58CE46AA4d
# USDT Arbitrum One:      0xFd086bC7CD5C481DCC9C85ebe478A1C0b69FCbb9
PAYMENT_TOKEN_ADDRESS=0x75faf114eafb1BDbe2F0316DF893fd58CE46AA4d

# iExec settings (relay)
# Strongly recommended: set a known working TEE app on your target network
# Arbitrum Sepolia example (validated in e2e): 0x8bda493af230369ed36d29c17e9eb7ed171acd55
IEXEC_TEE_APP=0x
IEXEC_APP_WHITELIST=0x
IEXEC_PROCESS_TIMEOUT_MS=420000
SHINGO_HUB_ADDRESS=0x
